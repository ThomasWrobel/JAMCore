package com.lostagain.Jam.SceneObjects;

/** the core types of scene objects, label is a subtype of dialogue **/
public enum SceneObjectType {
    /**
     * Everything is a sceneobject, but they may be other types as well
     */
	SceneObject,
	Sprite, 
	DialogBox, 
	Label,
	Div,
	Input, 
	/** inventory icons are a special type of sprite **/
	InventoryObject, 
	Vector

}